{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["last 2 versions", "not dead", "not < 2%", "not ie 11"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["react-hot-loader/babel", "@babel/plugin-syntax-dynamic-import"],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
